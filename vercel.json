{
    "version": 2,
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/api/test",
            "destination": "/api/test.js"
        },
        {
            "source": "/api/health",
            "destination": "/api/server.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ]
}