    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/server.js"
        }
    ],
    "functions": {
        "api/**": {
            "excludeFiles": "{vendor,agents/deep-research-repo,external}/**"
        }
    }
}