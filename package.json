{
    "name": "humanese-v3",
    "version": "1.0.0",
    "description": "Humanese - Universal Language Model with AI & Crypto",
    "type": "module",
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "vercel-build": "prisma generate && mkdir -p public && cp -r *.html css js assets agents data ts docs utils public/ 2>/dev/null || true",
        "build": "prisma generate",
        "autoheal": "node watcher_agent.js",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev --name init",
        "prisma:studio": "prisma studio",
        "postinstall": "prisma generate",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --write .",
        "test:testfox": "node testfox/index.js",
        "check-repos": "node scripts/repo_check.js"
    },
    "dependencies": {
        "@prisma/client": "^6.0.0",
        "prisma": "^6.0.0",
        "bcryptjs": "^3.0.3",
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^4.18.2",
        "express-rate-limit": "^8.2.1",
        "firebase-admin": "^13.6.1",
        "helmet": "^8.1.0",
        "jsdom": "^28.1.0",
        "jsonwebtoken": "^9.0.3",
        "node-fetch": "^3.3.2",
        "rss-parser": "^3.13.0",
        "twitter-api-v2": "^1.29.0",
        "vm2": "^3.10.5"
    },
    "devDependencies": {
        "concurrently": "^9.2.1",
        "nodemon": "^3.0.3"
    }
}